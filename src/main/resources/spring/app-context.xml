<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="datastoreService" class="com.google.appengine.api.datastore.DatastoreServiceFactory"
          factory-method="getDatastoreService"/>

    <bean id="memcacheService" class="com.google.appengine.api.memcache.MemcacheServiceFactory"
          factory-method="getAsyncMemcacheService"/>

    <bean id="requestManager" class="org.halkneistiyor.datamodel.gae.GaeDatastoreRequestManager"/>

    <bean id="userManager" class="org.halkneistiyor.datamodel.gae.GaeDatastoreUserManager" />

    <import resource="security-context.xml"/>
</beans>